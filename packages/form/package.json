{
  "name": "@sjsf/form",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "test": "vitest run --exclude '.svelte-kit/**'",
    "build": "svelte-package",
    "check": "svelte-check --tsconfig ./tsconfig.json && tsc -p tsconfig.node.json"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.5",
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.7",
    "@tsconfig/svelte": "^5.0.4",
    "@types/json-schema": "^7.0.15",
    "@types/json-schema-merge-allof": "^0.6.5",
    "@types/node": "^22.7.4",
    "ajv": "^8.17.1",
    "deep-freeze-es6": "^3.0.2",
    "json-schema": "^0.4.0",
    "svelte-check": "^4.0.4",
    "vite": "catalog:",
    "vite-plugin-dts": "^4.2.3",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "svelte": "5.0.0-next.260"
  },
  "dependencies": {
    "fast-equals": "^5.0.1",
    "json-schema-merge-allof": "^0.8.1",
    "jsonpointer": "^5.0.1",
    "scheduler-polyfill": "1.3.0-beta.0"
  },
  "exports": {
    "./lib/*": {
      "default": "./dist/lib/*.js",
      "svelte": "./dist/lib/*.js",
      "types": "./dist/lib/*.d.ts"
    },
    "./core": {
      "default": "./dist/core/export-only-do-not-use-internally.js",
      "svelte": "./dist/core/export-only-do-not-use-internally.js",
      "types": "./dist/core/export-only-do-not-use-internally.d.ts"
    },
    "./core/*": {
      "default": "./dist/core/*.js",
      "svelte": "./dist/core/*.js",
      "types": "./dist/core/*.d.ts"
    },
    "./core/schema": {
      "default": "./dist/core/schema/index.js",
      "svelte": "./dist/core/schema/index.js",
      "types": "./dist/core/schema/index.d.ts"
    },
    ".": {
      "default": "./dist/form/index.js",
      "svelte": "./dist/form/index.js",
      "types": "./dist/form/index.d.ts"
    },
    "./translations": {
      "default": "./dist/translations/index.js",
      "svelte": "./dist/translations/index.js",
      "types": "./dist/translations/index.d.ts"
    },
    "./translations/*": {
      "default": "./dist/translations/*.js",
      "svelte": "./dist/translations/*.js",
      "types": "./dist/translations/*.d.ts"
    },
    "./themes": {
      "default": "./dist/themes/index.js",
      "svelte": "./dist/themes/index.js",
      "types": "./dist/themes/index.d.ts"
    },
    "./themes/basic": {
      "default": "./dist/themes/basic/index.js",
      "svelte": "./dist/themes/basic/index.js",
      "types": "./dist/themes/basic/index.d.ts"
    },
    "./validators": {
      "default": "./dist/validators/index.js",
      "svelte": "./dist/validators/index.js",
      "types": "./dist/validators/index.d.ts"
    },
    "./validators/*": {
      "default": "./dist/validators/*.js",
      "svelte": "./dist/validators/*.js",
      "types": "./dist/validators/*.d.ts"
    }
  }
}