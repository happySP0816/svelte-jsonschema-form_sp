{
  "name": "@sjsf/ajv8-validator",
  "version": "1.1.0",
  "description": "The ajv-8 based validator for svelte-jsonschema-form",
  "license": "MIT",
  "keywords": [
    "validator",
    "jsonschema",
    "ajv8"
  ],
  "type": "module",
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "publishConfig": {
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/x0k/svelte-jsonschema-form.git",
    "directory": "packages/ajv8-validator"
  },
  "bugs": "https://github.com/x0k/svelte-jsonschema-form/issues",
  "homepage": "https://x0k.github.io/svelte-jsonschema-form/",
  "scripts": {
    "build": "tsc && publint",
    "dev": "tsc --watch"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "@sjsf/form": "workspace:^1.1.0",
    "ajv": "catalog:"
  },
  "devDependencies": {
    "@sjsf/form": "workspace:*"
  }
}